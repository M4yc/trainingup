{
  "expo": {
    "name": "trainingup",
    "slug": "trainingup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#000",
      "dark": {
        "image": "./assets/icons/splash-icon-dark.png",
        "backgroundColor": "#2A2634"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trainingup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#211d28"
      },
      "package": "com.trainingup"
    },
    "plugins": [
      "expo-dev-client",
      "expo-font",
      ["expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "newArchEnabled": true
  }
}
